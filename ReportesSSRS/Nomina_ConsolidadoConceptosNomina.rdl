  <DataSets>
    <DataSet Name="Nomina_ConsolidadoConceptosNomina">
      <Query>
        <DataSourceName>Principal</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FechaInicial">
            <Value>=Parameters!FechaInicial.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FechaFinal">
            <Value>=Parameters!FechaFinal.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ConceptoId">
            <Value>=Parameters!ConceptoId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
	DISTINCT cno.Id
	,cno.Nombre AS 'Concepto'
	,cno.ClaseConceptoNomina AS 'Clase'
	,COUNT(nfu.FuncionarioId) AS 'Número de funcionarios'
	,SUM(nde.Cantidad) AS 'Unidades'
	,SUM(nde.Valor) AS 'Netos'
	FROM dbo.NominaDetalle nde
	INNER JOIN dbo.NominaFuncionario nfu ON nde.NominaFuncionarioId = nfu.Id
	INNER JOIN dbo.Nomina nom ON nfu.NominaId = nom.Id
	INNER JOIN dbo.ConceptoNomina cno ON nde.ConceptoNominaId = cno.Id
	WHERE nom.Estado = (SELECT vce.NOMINA_APLICADA FROM util.VW_ConstanteEstado vce)
	AND nom.FechaInicio &gt;= @FechaInicial
	AND nom.FechaFinal &lt;= @FechaFinal
	AND ((@ConceptoId IS NOT NULL AND nde.ConceptoNominaId IN (SELECT * FROM util.UFT_SPLIT(@ConceptoId, ','))) OR (@ConceptoId IS NULL))
	GROUP BY cno.Id,cno.ClaseConceptoNomina,cno.Nombre
	ORDER BY cno.ClaseConceptoNomina DESC, cno.Nombre ;</CommandText>
      </Query>
      <Fields>
        <Field Name="Id">
          <DataField>Id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Concepto">
          <DataField>Concepto</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Clase">
          <DataField>Clase</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Número_de_funcionarios">
          <DataField>Número de funcionarios</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Unidades">
          <DataField>Unidades</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Netos">
          <DataField>Netos</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
