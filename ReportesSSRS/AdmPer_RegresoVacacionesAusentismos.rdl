  <DataSets>
    <DataSet Name="AdmPer_RegresoVacacionesAusentismos">
      <Query>
        <DataSourceName>Principal</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@EstadoFuncionario">
            <Value>=Parameters!EstadoFuncionario.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Dependencia">
            <Value>=Parameters!Dependencia.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CentroOperativo">
            <Value>=Parameters!CentroOperativo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Cargo">
            <Value>=Parameters!Cargo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT fun.Id AS 'Id', 
       tdo.Nombre AS 'Tipo de documento', 
       fun.NumeroDocumento AS 'Número de documento', 
       fun.PrimerApellido AS 'Primer apellido', 
       fun.SegundoApellido AS 'Segundo apellido', 
       fun.PrimerNombre AS 'Primer nombre', 
       fun.SegundoNombre AS 'Segundo nombre', 
       car.Nombre AS 'Cargo', 
       dep.Nombre AS 'Dependencia', 
       ceo.Nombre AS 'CentroOperativo', 
       fun.Estado AS 'Estado del funcionario', 
       afu.FechaInicio AS 'Fecha inicial', 
       afu.FechaFin AS 'Fecha final', 
       DATEADD(DAY, 1, afu.FechaFin) AS 'Fecha de regreso'
FROM dbo.Funcionario fun
     INNER JOIN dbo.VW_FuncionarioDatoActual vfda ON fun.Id = vfda.Id
     INNER JOIN dbo.Cargo car ON vfda.CargoId = car.Id
     INNER JOIN dbo.Dependencia dep ON vfda.DependenciaId = dep.Id
     INNER JOIN dbo.TipoDocumento tdo ON vfda.TipoDocumentoId = tdo.Id
     INNER JOIN dbo.CentroOperativo ceo ON vfda.CentroOperativoId = ceo.Id
     INNER JOIN dbo.AusentismoFuncionario afu ON fun.Id = afu.FuncionarioId
WHERE(afu.Estado =
(
    SELECT vce.AUSENTISMO_APROBADO
    FROM util.VW_ConstanteEstado vce
)
      AND fun.Estado IN
(
    SELECT *
    FROM util.UFT_SPLIT(@EstadoFuncionario, ',')
)
     AND ((@Dependencia IS NOT NULL
           AND dep.Id IN
(
    SELECT *
    FROM util.UFT_SPLIT(@Dependencia, ',')
))
OR (@Dependencia IS NULL))
AND ((@CentroOperativo IS NOT NULL
      AND ceo.Id IN
(
    SELECT *
    FROM util.UFT_SPLIT(@CentroOperativo, ',')
))
OR (@CentroOperativo IS NULL))
AND ((@Cargo IS NOT NULL
      AND car.Id IN
(
    SELECT *
    FROM util.UFT_SPLIT(@Cargo, ',')
))
OR (@Cargo IS NULL)))
UNION

SELECT fun.Id AS 'Id', 
       tdo.Nombre AS 'Tipo de documento', 
       fun.NumeroDocumento AS 'Número de documento', 
       fun.PrimerApellido AS 'Primer apellido', 
       fun.SegundoApellido AS 'Segundo apellido', 
       fun.PrimerNombre AS 'Primer nombre', 
       fun.SegundoNombre AS 'Segundo nombre', 
       car.Nombre AS 'Cargo', 
       dep.Nombre AS 'Dependencia', 
       ceo.Nombre AS 'CentroOperativo', 
       fun.Estado AS 'Estado del funcionario', 
       sva.FechaInicioDisfrute AS 'Fecha inicial', 
       sva.FechaFinDisfrute AS 'Fecha final', 
       sva.FechaRegreso AS 'Fecha de regreso'
FROM dbo.Funcionario fun
     INNER JOIN dbo.VW_FuncionarioDatoActual vfda ON fun.Id = vfda.Id
     INNER JOIN dbo.Cargo car ON vfda.CargoId = car.Id
     INNER JOIN dbo.Dependencia dep ON vfda.DependenciaId = dep.Id
     INNER JOIN dbo.TipoDocumento tdo ON vfda.TipoDocumentoId = tdo.Id
     INNER JOIN dbo.CentroOperativo ceo ON vfda.CentroOperativoId = ceo.Id
     INNER JOIN dbo.SolicitudVacaciones sva ON fun.Id = sva.FuncionarioId
WHERE((sva.Estado =
(
    SELECT vce.SOLICITUDVACACIONES_ENCURSO
    FROM util.VW_ConstanteEstado vce
) OR sva.Estado =
(
    SELECT 
           vce.SOLICITUDVACACIONES_AUTORIZADA
    FROM util.VW_ConstanteEstado vce
) )
      AND fun.Estado IN
(
    SELECT *
    FROM util.UFT_SPLIT(@EstadoFuncionario, ',')
)
     AND ((@Dependencia IS NOT NULL
           AND dep.Id IN
(
    SELECT *
    FROM util.UFT_SPLIT(@Dependencia, ',')
))
OR (@Dependencia IS NULL))
AND ((@CentroOperativo IS NOT NULL
      AND ceo.Id IN
(
    SELECT *
    FROM util.UFT_SPLIT(@CentroOperativo, ',')
))
OR (@CentroOperativo IS NULL))
AND ((@Cargo IS NOT NULL
      AND car.Id IN
(
    SELECT *
    FROM util.UFT_SPLIT(@Cargo, ',')
))
OR (@Cargo IS NULL)))
ORDER BY 'Primer nombre';</CommandText>
