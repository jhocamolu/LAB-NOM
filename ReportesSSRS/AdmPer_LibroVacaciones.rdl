  <DataSets>
    <DataSet Name="AdmPer_LibroVacaciones">
      <Query>
        <DataSourceName>Principal</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Periodo">
            <Value>=Parameters!Periodo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FuncionarioId">
            <Value>=Parameters!FuncionarioId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EstadoContrato">
            <Value>=Parameters!EstadoContrato.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DependenciaId">
            <Value>=Parameters!DependenciaId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CentroOperativoId">
            <Value>=Parameters!CentroOperativoId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>    DECLARE @Periodos VARCHAR(255);
    DECLARE @Tipo VARCHAR(255)= NULL;
    DECLARE @DiasDisponibles VARCHAR(10)= NULL;

    DECLARE CUR_Periodo CURSOR LOCAL
    FOR SELECT *
        FROM util.UFT_SPLIT(@Periodo, ',');
    OPEN CUR_Periodo;
    FETCH NEXT FROM CUR_Periodo INTO @Periodos;
    WHILE @@fetch_status = 0
    BEGIN

        IF @Periodos = 'CausadosTomados'
        BEGIN
            SET @Tipo = 'Causado';
            SET @DiasDisponibles = '=';
        END;

        IF @Periodos = 'CausadosPendientes'
        BEGIN
            SET @Tipo = 'Causado';
            SET @DiasDisponibles = '&lt;&gt;';
        END;

        IF @Periodos = 'AnticipadosTomados'
        BEGIN
			SET @DiasDisponibles = null;
            IF @Tipo IS NULL
            BEGIN
                SET @Tipo = 'Anticipado';
            END;
            ELSE
            BEGIN
                SET @Tipo = 'Causado,Anticipado';
            END;
        END;

        FETCH NEXT FROM CUR_Periodo INTO @Periodos;
    END;

    CLOSE CUR_Periodo;
    DEALLOCATE CUR_Periodo;

SELECT
lva.Id,
con.FuncionarioId,
tdo.Nombre AS 'Tipo de documento',
           fun.NumeroDocumento AS 'Número de documento',
           fun.PrimerApellido AS 'Primer apellido',
           fun.SegundoApellido AS 'Segundo apellido',
           fun.PrimerNombre AS 'Primer nombre',
           fun.SegundoNombre AS 'Segundo nombre',
  car.Nombre AS 'Cargo',
  dep.Nombre AS 'Dependencia',
  ceo.Nombre AS 'Centro Operativo',
  ISNULL(sva.FechaInicioDisfrute, NULL) AS 'Fecha inicio período vacaciones',
  ISNULL(sva.FechaFinDisfrute , NULL) AS 'Fecha fin periodo vacaciones',
  sva.DiasDisfrute AS 'Vacaciones en tiempo',
  sva.DiasDinero AS 'Vacaciones en dinero',
  sva.DiasDisfrute + sva.DiasDinero AS 'Días vacaciones',
  lva.DiasDisponibles AS 'Días pendientes por tomar'
FROM dbo.LibroVacaciones lva
LEFT JOIN dbo.SolicitudVacaciones sva ON lva.Id = sva.LibroVacacionesId
LEFT JOIN dbo.Contrato con ON lva.ContratoId = con.Id
LEFT JOIN dbo.Funcionario fun ON con.FuncionarioId = fun.Id
INNER JOIN dbo.CargoDependencia cde ON con.CargoDependenciaId = cde.Id
INNER JOIN dbo.Cargo car ON cde.CargoId = car.Id
INNER JOIN dbo.Dependencia dep ON cde.DependenciaId = dep.Id
INNER JOIN dbo.TipoDocumento tdo ON fun.TipoDocumentoId = tdo.Id
INNER JOIN dbo.CentroOperativo ceo ON con.CentroOperativoId = ceo.Id
WHERE lva.Tipo IN (SELECT * FROM util.UFT_SPLIT(@Tipo, ','))
AND ((@DiasDisponibles IS NOT NULL AND  @DiasDisponibles = '=' AND lva.DiasDisponibles = 0 )  OR (@DiasDisponibles IS NOT NULL AND  @DiasDisponibles = '&lt;&gt;' AND lva.DiasDisponibles &lt;&gt; 0 ) OR (@DiasDisponibles IS NULL) )
AND ((@FuncionarioId IS NOT NULL AND con.FuncionarioId = @FuncionarioId ) OR (@FuncionarioId IS NULL) )
AND ((@EstadoContrato IS NOT NULL AND con.Estado IN (SELECT * FROM util.UFT_SPLIT(@EstadoContrato, ','))) OR (@EstadoContrato IS NULL))
AND ((@DependenciaId IS NOT NULL AND dep.Id IN (SELECT * FROM util.UFT_SPLIT(@DependenciaId, ','))) OR (@DependenciaId IS NULL))
AND ((@CentroOperativoId IS NOT NULL AND ceo.Id IN (SELECT * FROM util.UFT_SPLIT(@CentroOperativoId, ','))) OR (@CentroOperativoId IS NULL))
ORDER BY  fun.PrimerNombre, sva.FechaInicioDisfrute;</CommandText>
      </Query>
